{
  "name": "path-crawler",
  "version": "0.0.0",
  "description": "An utility to crawl generic objects paths",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run cjs && npm run test",
    "cjs": "ascjs esm cjs",
    "coveralls": "c8 report --reporter=text-lcov | coveralls",
    "test": "c8 node test/index.js"
  },
  "keywords": [
    "json",
    "object",
    "path",
    "crawler"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "ascjs": "^5.0.1",
    "c8": "^7.9.0",
    "coveralls": "^3.1.1"
  },
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/path-crawler.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/path-crawler/issues"
  },
  "homepage": "https://github.com/WebReflection/path-crawler#readme"
}
